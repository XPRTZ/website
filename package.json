{
  "name": "xprtz-websites",
  "private": true,
  "version": "0.1.0",
  "author": "Alex Schouls",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "format": "prettier ./**/*.{js,ts,html,css,scss,json,mjs} --write && eslint ./**/*.{js,ts,mjs}",
    "dev:dotnet": "pnpm run --filter ./apps/dotnet dev",
    "dev:cloud": "pnpm run --filter ./apps/cloud dev",
    "dev": "pnpm run --filter ./apps/* --if-present dev",
    "build": "pnpm run --filter ./libs/* --if-present build"
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.3.0",
    "prettier": "3.2.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.0.0-alpha.20"
  },
  "packageManager": "pnpm@9.1.3"
}
