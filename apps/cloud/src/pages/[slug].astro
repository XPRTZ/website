---
import { fetchData } from "@xprtz/cms";
export async function getStaticPaths() {
  const pageData = await fetchData<Array<object>>({
    endpoint: "pages",
    wrappedByKey: "data",
    query: {
      "filters[site][$eq]": "dotnet",
    },
  });
  return pageData.map((data: any) => ({
    params: { slug: data.slug },
    props: data,
  }));
}

const page: any = Astro.props;
---

<h1>{page.title}</h1>
<h2>{page.description}</h2>
